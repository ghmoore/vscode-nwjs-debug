{
    "version": "0.1.0",
    "configurations": [
        {
            "debugServer": 4712,
            "name": "test chrome",
            "type": "chrome",
            "request": "launch",
            "url": "http://localhost:8080/index.html",
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}/wwwroot",
            "userDataDir": "${workspaceRoot}/.vscode/chrome"
        },
        {
            "name": "attach to chrome",
            "type": "chrome",
            "port": 9222,
            "request": "attach",
            "sourceMaps": true,
            "diagnosticLogging": true,
            "webRoot": "${workspaceRoot}/wwwroot",
            "url": "http://localhost:8080*"
        }
    ]
}
